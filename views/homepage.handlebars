<!-- Loop over each object in the projects array -->
{{#each cmss as |cms| }}
    <div class="row mb-4 project">
      <div class="col-md-12">
          <!-- Call helper function with triple brackets to render the emoji span as HTML -->
        <a href="/cms/{{cms.id}}">
          <h6>{{cms.title}} Posted by: {{cms.user.name}} on {{format_date cms.date_created}}</h6>
        </a>
          <span class="col-md-12">{{cms.content}}</span>
      </div>
      <div class="col-md-7">
        <p>
        </p>
      </div>
    </div>
{{/each}}
